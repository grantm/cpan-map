#!/usr/bin/perl
#
# This script generates the JSON file and PNG map image used by the
# HTML/Javascript map frontend.
#

use strict;
use warnings;

use CPAN::Map;

CPAN::Map->build(
    verbose         => 1,
    label_font_path => '/usr/share/fonts/truetype/msttcorefonts/Arial_Bold.ttf',
);

exit;

